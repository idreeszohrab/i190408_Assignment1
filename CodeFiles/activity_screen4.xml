<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Screen4"
    android:background="@color/black">

    <ImageView
        android:layout_width="183dp"
        android:layout_height="203dp"
        android:src="@drawable/musify"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.096"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-2dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="421dp"
        android:layout_height="52dp"
        android:background="@drawable/name"
        android:gravity="center"
        android:textColor="#FF3F51B5"
        android:text="Continue with Facebook"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.427"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-5dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="421dp"
        android:layout_height="52dp"
        android:background="@drawable/name"
        android:gravity="center"
         android:textColor="#FFF81100"
        android:text="Continue with Google"
        android:textColorHint="#F81100"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.561"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-2dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="421dp"
        android:layout_height="52dp"
        android:background="@drawable/name"
        android:gravity="center"
        android:hint="Create Account"
        android:textColorHint="#FFC107"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.698"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-5dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="407dp"
        android:layout_height="44dp"
        android:background="#FFFFF"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="477dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button2"
        android:layout_width="407dp"
        android:layout_height="44dp"
        android:layout_marginStart="8dp"
        android:background="#FFFFF"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="298dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/facebook"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="290dp"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/google"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="381dp"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>